{
  "name": "tiktok-scraper-tool",
  "version": "1.0.0",
  "description": "TikTok video scraper with web UI and automation",
  "main": "server.js",
  "scripts": {
    "build": "tsc",
    "start": "node server.js",
    "dev": "ts-node src/main.ts",
    "dev:real": "ts-node src/main.ts --real",
    "dev:clean": "ts-node src/main.ts --real --no-proxy",
    "dev:firefox": "ts-node src/main.ts --firefox --no-proxy",
    "dev:edge": "ts-node src/main.ts --edge --no-proxy",
    "ui": "node server.js",
    "watch": "tsc --watch",
    "scrape": "ts-node src/main.ts"
  },
  "keywords": [
    "tiktok",
    "scraper",
    "automation",
    "puppeteer",
    "typescript",
    "web-ui"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "puppeteer": "^24.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.22",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
